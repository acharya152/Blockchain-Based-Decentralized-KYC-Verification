@using Microsoft.AspNetCore.Identity
@using Fyp.Areas.Identity.Data

@inject SignInManager<Fypuser> SignInManager
@inject UserManager<Fypuser> UserManager


@if (SignInManager.IsSignedIn(User))
{
   <div class="navbar">

        <a id="home"class="nav-link text-dark" asp-page="/Views/Home/Index" title="Manage">Fyp.</a>
        <a id="manage" class="nav-link text-dark" asp-area="Identity" asp-page="/Account/Manage/Index" title="Manage"><img id="logo4" src="~/icon.png"/></a>
    
   
        <form id="logoutForm" class="form-inline" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home", new { area = "" })">
            <button type="submit" class="btn  btn-danger">Logout</button>
             
        </form>
       


    </div>
           

   
}


